<head>
  {% if template.name == 'product' %} 
    {% if product.handle contains 'navidium' %}
      <script>
        window.location.replace('/');
      </script>
    {% endif %} 
  {% endif %} 

  <style>
    [data-handle*="navidium"]{
      display: none !important;
    }
  </style>
  {% include 'pro-blogger-section-wrapper' %}
  {%- assign usedcss_enabled = true -%}
  {%- assign is_shared_page = false -%}
  {% assign shared_urls = '/pages/premium-cigars,/pages/pipe-tobacco,/pages/ryo-supplies,/pages/little-cigars,/pages/machine-made-cigars,' | split: ',' %}
  {% if shared_urls contains page.url %}
        {% assign is_shared_page = true %}
  {%- endif -%}
  {%- if usedcss_enabled == false -%}
  <link rel="preload" href="{{ 'core.min.scss' | asset_url }}" as="style">
  <link rel="preload" href="{{ 'services.scss.css' | asset_url }}" as="style">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

  {%- endif -%}
  <link rel="preconnect" href="https://yotpo-editor-production.s3.amazonaws.com">
  {%- assign yotpo_offload_content = shop.metafields.yotpo.yotpo_offload_content %}
  {%- assign time_now = 'now' | date: '%s' %}
  {%- assign yotpo_live_time = shop.metafields.yotpo.yotpo_live | date: '%s' %}
  {%- assign diffSeconds = time_now | minus: yotpo_live_time %}
  {%- if yotpo_offload_content == 'yes' and diffSeconds < 86400 -%}

  <link rel="preload" href="{{ 'yotpo-full-css.css' | asset_url }}" as="style">
  
  {%- endif -%}
  <!-- Start SpeedCurve tracking code -->
  <script>LUX = window.LUX || {}; LUX.samplerate = 100;</script>
  <script src="https://cdn.speedcurve.com/js/lux.js?id=142213516" async defer></script>
  <!-- End SpeedCurve tracking code -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  {% if settings.favicon_enable %}
  
  <link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" type="image/png" />
  
  {% endif %}
  <title>
    {{ page_title }}{% if product and collection %} &#124; {{ collection.title }}{% endif %}{% if current_tags %}{% assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains 'BNB Tobacco' or page_title contains shop.name %} &#124; {{ shop.name }}{% endunless %}
  </title>

  {% if page_description %}
  <meta name="description" content="{{ page_description | escape }}">
  {% endif %}


  <!-- Social meta ================================================== -->
  {% include 'social-meta-tags' %}

  <!-- Helpers ================================================== -->
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta name="theme-color" content="{{ settings.color_primary }}">
  {%comment%}<link rel="preload" href="{{ 'custom.css.liquid' | asset_url }}" as="style">{%endcomment%}

  <!-- CSS -->
  {% if template == 'index' %}
  {% include 'css-front' %}
  {% elsif template.name == 'collection' %}
  {% include 'css-collection' %}
  {% elsif template.name == 'product' %}
  {% include 'css-product' %}
  {% elsif is_shared_page %}
  {%- include 'css-shared' -%}
  {% else %}
  
  {{ 'core.min.scss' | asset_url | stylesheet_tag }}
  {{ 'services.scss.css' | asset_url | stylesheet_tag }}
  
  {% endif %}
  
  <script id="raf">var raf; try{raf = requestAnimationFrame || mozRequestAnimationFrame || webkitRequestAnimationFrame || msRequestAnimationFrame;} catch(e){ raf = function(cb){window.addEventListener('load', cb);}};</script>
  {% comment %}{{ 'core.scss.css' | asset_url | stylesheet_tag }}{% endcomment %}
  <style>
    #bc-sf-filter-products a {
    	max-height: 100% !important;
      	width: auto !important;
    }
    #bc-sf-filter-products a img {
      	width: auto !important;
    }
    
    .rte.clearfix.truncated {
    	display: none;
    }
    
    @media(max-width: 500px) {
      .rte.clearfix.truncated {
          display: block;
      }
      .rte.clearfix.truncated.opened {
          display: none;
      }
      
      .truncated + .rte.clearfix p {
      		display: none;
      }
      
      .truncated + .rte.clearfix p.show {
      		display: block
      }

    }
    
    ul.horizontal-mega-menu li.mega-menu-item:nth-child(2)>ul.mm-submenu {
    	max-height: none !important;
	}
    ul.horizontal-mega-menu li.mega-menu-item:nth-child(5)>ul.mm-submenu {
    	max-height: none !important;
	}
    
    .horizontal-mega-menu .mega-menu-item:hover ul.mm-submenu {
    	flex-direction: row !important;
	}

    .footer-container a {
      color: #d08305 !important;
    }
    .footer-container a:hover {
      color: #8e5903 !important;
    }

    .shopify-section.featured-products .bg-hook {
      padding: 25px 25px 25px;
    }
    .featured-products .product-grid-item .grid-link__image {
      width: 65%;
      margin: 0 auto;
      display: block;
      padding: 10px;
      border: 0px solid #dedede; 
      border-bottom: 0;
      border-radius: 5px 5px 0 0;
    }

    .view-all {
      margin: 0 0 15px;
    } 

    .header-checkout-button {
      background: #d08305;
      color: #fff;
      padding: 14px 9px 12px;
      border-radius: 5px;
      margin-left: 5px;
      font-family: "trajan pro";
      font-size: 12px;
      line-height: 12px;
      display: inline-block;
      vertical-align: middle;
      position: relative;
      top: 19px;
    }
    .header-bar .cigar-devil-link {
      display: block;
      float: left;
      margin-right: 10px;
    }
    .header-bar .cigar-devil-link img, .mobile-menu-expand .mobile-phone .cigar-devil-link img {
      position: relative;
      top: 2px;
      height: 35px;
    }
    .mobile-menu-expand .mobile-phone {
      padding: 0;
    }
    .mobile-menu-expand .mobile-phone > div {
      padding: 10px;
      display: flex;
    }
    .mobile-menu-expand .mobile-phone svg {
      margin-right: 8px;
    }
    .mobile-menu-expand .mobile-phone strong {
      margin-left: 8px;
    }
    .mobile-menu-expand .cigar-devil-link img {
      max-width: 110px;
      margin: 10px 15px;
    }
    .supports-fontface .site-header .header-search-box .header-bar__search-submit{
      background:#D08305!important;
      width:auto !important;
      border-top-right-radius: 5px !important;
      border-bottom-right-radius: 5px !important;
      display: inline-block !important;
      font-size: 12px !important;
      font-weight: 600;
      position:absolute;
      right:0;
      height:40px!important;
      padding: 6px 12px;
      font-family: 'TrajanProBold';
      text-transform: none;
      opacity:1;
      transition: opacity .5s;
    }
    .supports-fontface .site-header .header-search-box .header-bar__search-submit:hover{
      background:#C67B0C!important;
    }
    .site-header .header-search-box .search-button{
      margin-left: 20px !important;
    }
    @media only screen and (max-width: 1279px) {
      .header-bar .cigar-devil-link {display: none;}
    }
/*     body.mobile-search-active{overflow:hidden;} */
    body.mobile-search-active .search-overlay {
      opacity: 1;
      background: rgba(0,0,0,.5);
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: 5;
    }
    .mobile-search {
      z-index: 99999 !important;
    }
    @media (max-width:767px){
      .algolia-autocomplete{
        top:15vh !important;
        height:80vh !important;
        overflow:scroll;
        padding-bottom: 50px;
      }
      .header-bar__search-submit{
        width: 100px !important;
      }
      .mobile-search-active #gorgias-web-messenger-container{display:none;}
    }
    @media (max-width:1279px){
      .header-bar__search-submit{
        width: 100px !important;
      }
    }
    @media (min-width:1280px){
      .algolia-autocomplete{
        top:178px !important;
      }
    }
    .cart-drawer.empty .items-container{text-align:center;padding:1rem}
  </style>
