<script src="{{ 'modernizr.min.js' | asset_url }}" async type="text/javascript"></script>
 

  {% comment %}
  If you store has customer accounts disabled, you can remove the following JS file
  {% endcomment %}
  {% if template contains 'customers' %}
  <script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer="defer" type="text/javascript"></script>
  {% endif %}

  <!-- NOINDEX or NOFOLLOW for BETA Launch -- Remove after site goes live -->
  <!--<meta name ="robots" content="noindex,nofollow"/>-->

  {% if template contains 'collection' and current_tags %} 
  <meta name="robots" content="noindex" /> 
  <link rel="canonical" href="{{ shop.url }}{{ collection.url }}" /> 
  {% elsif template contains 'collection' and collection.current_type %}
  <meta name="robots" content="noindex" /> 
  <link rel="canonical" href="{{ shop.url }}{{ collection.url }}" /> 
  {% elsif template contains 'collection' and collection.current_vendor %}
  <meta name="robots" content="noindex" /> 
  <link rel="canonical" href="{{ shop.url }}{{ collection.url }}" />
  {% elsif template contains 'search?' %}
  <meta name="robots" content="noindex" /> 
  {% elsif template contains 'article' %}
  <link rel="canonical" href="{{ shop.url }}{{ article.url }}" /> 
  {% else %} 
  <link rel="canonical" href="{{ canonical_url }}" /> 
  {% endif %}

  <!--yotpo-->
  <script src="{{ 'yotpo-inline.js' | asset_url }}" defer="defer" type="text/javascript"></script>  
  <!--yotpo-->
  {% if product.metafields.inventory.ShappifyHidden == "true" %} <meta name="robots" content="noindex"> {% endif %}
  <!--   <script type='text/javascript' src='https://filters.eshopadmin.com/static/js/plain/esafilters.js'></script> -->
  <script src="{{ 'esafilters.js' | asset_url }}" {% if template == 'collection' %}{% else %}defer="defer"{% endif %} type="text/javascript"></script>
  <script src="{{ 'jquery.bxslider.min.js' | asset_url }}" type="text/javascript" defer="defer"></script>
  

  {% include 'bold-common' %}
  {% include 'bold-product', hide_action: 'header' %}
  {%- include 'bold-product' with product, hide_action: 'header' -%}
  

  <!-- Algolia head -->

  <script type="text/template" id="template_algolia_money_format">{% include 'algolia_money_format' %}</script>
  <script type="text/template" id="template_algolia_autocomplete">{% include 'algolia_autocomplete.hogan' %}</script>
  <script type="text/template" id="template_algolia_autocomplete.css">{% include 'algolia_autocomplete.css.hogan' %}</script>
  <script type="text/template" id="template_algolia_autocomplete_pages_empty">{% include 'algolia_autocomplete_pages_empty.hogan' %}</script>
  <script type="text/template" id="template_algolia_autocomplete_page">{% include 'algolia_autocomplete_page.hogan' %}</script>
