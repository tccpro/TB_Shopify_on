{{ 'https://cdnjs.cloudflare.com/ajax/libs/pikaday/1.4.0/css/pikaday.min.css' | stylesheet_tag }}
{{ 'https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js' | script_tag }}
{{ 'https://cdnjs.cloudflare.com/ajax/libs/pikaday/1.4.0/pikaday.min.js' | script_tag }}
{{ 'https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js' | script_tag }}

<label for="Birthday">Birthday - (Must be 21 and over to purchase) </label>
<input type="text" name="customer[note][Birthday]" required="required" id="Birthday" placeholder="MM/DD/YYYY" autocomplete="off" />

<style>
  .evs-theme .pika-title .pika-label {
    font-size: 0;
  }
  .evs-theme .pika-title .pika-select {
	background: transparent url('data:image/png;base64,') scroll no-repeat 100% center;
	font-size: 14px;
	position: static;
	padding: 5px 20px 5px 5px;
	margin: 0;
	opacity: 1;
	text-indent: 0.01px;
	-moz-appearance: none;
	-webkit-appearance: none;
	appearance: none;
  }
</style>

<script>
  (function () {
	var $birthday = $('#Birthday');

    var picker = new Pikaday({
      field: $birthday.get(0),
      format: 'MM/DD/YYYY',
      maxDate: moment(),
	  theme: 'evs-theme',
      yearRange: [1900, moment().get('year')],
      onSelect: function () {
        $birthday.removeClass('error');
      }
    });

    $birthday.on('focus', function () {
      $birthday.removeClass('error');
    });

	$birthday.on('keyup', function (e) {
	  var day, month, year, match;
	  var value = $birthday.val();
	  var currentDate = picker.getDate() || new Date();

	  match = value.match(/(\d{1,2})\/(\d{1,2})\/(\d{1,4})/);
	  if (match !== null) {
	    day = parseInt(match[2]);
		month = parseInt(match[1]) - 1;
		year = parseInt(match[3] + new Array(5 - match[3].length).join('0'));

		picker.gotoDate(new Date(year, month, day));
		return true;
	  }

	  match = value.match(/(\d{1,2})\/(\d{1,2})\/?/);
	  if (match !== null) {
	    day = parseInt(match[2]);
		month = parseInt(match[1]) - 1;
		year = currentDate.getFullYear();

		picker.gotoDate(new Date(year, month, day));
		return true;
	  }

	  match = value.match(/(\d{1,2})\/?/);
	  if (match !== null) {
	    day = currentDate.getDay();
		month = parseInt(match[1]);
		year = currentDate.getFullYear();

		picker.gotoDate(new Date(year, month, day));
		return true;
	  }

	  return true;
	});

	$birthday.closest('form').on('submit', function (e) {
	  var $form = $(this);
      var $errors = $form.find('.errors');
      var $errorsUl = $errors.find('ul');
	  var errorHtml;

	  if (!moment($birthday.val(), 'MM/DD/YYYY').isValid()) {
        $birthday.addClass('error');
        errorHtml = '<li class="date-error">Please enter a valid birthday</li>';
        if ($errors.find('.date-error').length === 0) {
          if ($errors.length === 0) {
            $form.prepend('<div class="errors"><ul>'+errorHtml+'</ul></div>');
          } else if ($errorsUl.length === 0) {
            $errors.append('<ul>'+errorHtml+'</ul>');
          } else {
            $errorsUl.append(errorHtml);
          }
        }
        return false;
      }

	  $errorsUl.find('.date-error').remove();

	  var years = moment().diff($birthday.val(), 'years');
      if (years < 18) {
        $birthday.addClass('error');
        errorHtml = '<li class="age-error">You must be at least 18 years old to register</li>';
        if ($errors.find('.age-error').length === 0) {
          if ($errors.length === 0) {
            $form.prepend('<div class="errors"><ul>'+errorHtml+'</ul></div>');
          } else if ($errorsUl.length === 0) {
            $errors.append('<ul>'+errorHtml+'</ul>');
          } else {
            $errorsUl.append(errorHtml);
          }
        }
        return false;
      }

	  $errorsUl.find('.age-error').remove();

      return true;
    });
  })();
</script>